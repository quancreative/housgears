<script>
  document.querySelectorAll('.star-rating-overlay').forEach(el => {
    const score = parseFloat(el.dataset.score) || 0;
    const max = parseFloat(el.dataset.max) || 5;
    const percent = Math.max(0, Math.min(100, (score / max) * 100));

    // update gradient offsets
    const stops = el.querySelectorAll('stop');
    if (stops.length >= 4) {
      stops[1].setAttribute('offset', percent + '%');
      stops[2].setAttribute('offset', percent + '%');
    }

    // update overlay text
    const text = el.querySelector('.star-text');
    if (text) text.textContent = score.toFixed(1);
  });

</script>
      
